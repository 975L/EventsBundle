{% apply spaceless %}
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for event in events %}
        <url>
            <loc>{{ event.url }}</loc>
            <changefreq>{{ event.changeFrequency }}</changefreq>
            <priority>{{ event.priority }}</priority>
        </url>
    {% endfor %}
</urlset>
{% endapply %}