{
    "name": "c975l/events-bundle",
    "type": "symfony-bundle",
    "description": "Tool to manage events and display a carousel",
    "keywords": ["symfony", "bundle", "twig", "events"],
    "homepage": "https://github.com/975L/EventsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/LaurentMarquet",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "c975l/config-bundle": "^2",
        "c975l/includelibrary-bundle": "^1",
        "c975l/services-bundle": "^1",
        "c975l/toolbar-bundle": "^1",
        "cocur/slugify": "^3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^2",
        "knplabs/knp-paginator-bundle": "^4",
        "symfony/form": "*",
        "symfony/security": "*",
        "twig/twig": "*",
        "twig/intl-extra": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "rector/rector": "^0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\EventsBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/EventsBundle",
        "issues": "https://github.com/975L/EventsBundle/issues"
    }
}
